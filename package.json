    "eject": "react-scripts eject",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss,md,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,scss,md,json}\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
